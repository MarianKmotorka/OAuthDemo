{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "OAuthDemoDb": "Server=.;Database=OAuthDemoDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Description": "Dormitory API",
    "UiEndpoint": "v1/swagger.json"
  },

  "JwtOptions": {
    "Secret": "secret_used_to_sign_a_jwt_token_so_it_is_not_forged",
    "TokenLifetime": "00:10:00",
    "Issuer": "Some_Valid_Issuer"
  },

  // Name GoogleOAuthOptions
  "OAuthOptions": {
    "GoogleClientId": "your_client_id",
    "GoogleClientSecret": "your_client_secret",
    "TokenEndpoint": "https://oauth2.googleapis.com/token",
    "UserInfoEndpoint": "https://www.googleapis.com/oauth2/v2/userinfo"
  }
}
